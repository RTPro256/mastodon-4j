<?xml version="1.0" encoding="UTF-8"?>
<!--
  OWASP Dependency Check Suppressions
  
  This file contains suppressions for known false positives or 
  accepted risks in dependency vulnerability scanning.
  
  Documentation: https://jeremylong.github.io/DependencyCheck/general/suppression.html
-->
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Example: Suppress false positive for Spring Boot Starter -->
    <!--
    <suppress>
        <notes>False positive - this vulnerability only affects Spring when used with specific configurations</notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-starter-.*@.*$</packageUrl>
        <cve>CVE-XXXX-XXXXX</cve>
    </suppress>
    -->
    
    <!-- Suppress vulnerabilities in test dependencies -->
    <suppress>
        <notes>Test dependency - not shipped in production</notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*@.*$</packageUrl>
        <cpe>cpe:/a:testcontainers:testcontainers</cpe>
    </suppress>
    
    <!-- H2 Database - development only -->
    <suppress>
        <notes>H2 is only used in development profile, not production</notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
        <cpe>cpe:/a:h2database:h2</cpe>
    </suppress>

</suppressions>
